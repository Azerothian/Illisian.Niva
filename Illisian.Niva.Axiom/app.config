<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="axiom" type="Axiom.Framework.Configuration.AxiomConfigurationSection, Axiom.Framework"/>
	</configSections>
	<axiom xmlns="http://www.axiom3d.net/schemas/configuration/v1.0">
		<resourceLocations>
			<resourceLocation path="../../media/Materials" type="Folder" recurse="false"
        group="Data" />
			<resourceLocation path="../../media/Textures" type="Folder" recurse="false"
        group="Data" />
		</resourceLocations>
		<renderSystems>
			<renderSystem name="Xna">
				<options>
					<option name="Rendering Device" value="NVIDIA NVS 3100M " />
					<option name="Video Mode" value="1280 x 1050 @ 32-bit color" />
					<option name="Full Screen" value="Yes" />
					<option name="VSync" value="No" />
					<option name="Anti aliasing" value="None" />
					<option name="Floating-point mode" value="Fastest" />
					<option name="Allow NVPerfHUD" value="No" />
					<option name="Save Generated Shaders" value="No" />
					<option name="Use Content Pipeline" value="No" />
				</options>
			</renderSystem>
			<renderSystem name="DirectX9">
				<options>
					<option name="Rendering Device" value="ATI Radeon HD 4800 Series" />
					<option name="Video Mode" value="1024 x 768 @ 32-bit color" />
					<option name="Full Screen" value="No" />
					<option name="VSync" value="No" />
					<option name="Anti aliasing" value="None" />
					<option name="Floating-point mode" value="Fastest" />
					<option name="Allow NVPerfHUD" value="No" />
				</options>
			</renderSystem>
			<renderSystem name="OpenGL">
				<options>
					<option name="Video Mode" value="1280 x 1050" />
					<option name="Color Depth" value="32" />
					<option name="Display Frequency" value="75" />
					<option name="Full Screen" value="Yes" />
					<option name="FSAA" value="0" />
					<option name="VSync" value="No" />
					<option name="RTT Preferred Mode" value="FBO" />
				</options>
			</renderSystem>
		</renderSystems>
	</axiom>



	<startup useLegacyV2RuntimeActivationPolicy="true">
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
</configuration>
